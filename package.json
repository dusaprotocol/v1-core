{
    "name": "@dusalabs/core",
    "version": "0.0.1",
    "description": "Core contracts of Dusa Protocol",
    "scripts": {
        "test": "asp --summary",
        "build": "tsx src/compile.ts",
        "build:main": "asc assembly/contracts/main.ts -o build/main.wasm --transform @massalabs/as-transformer",
        "deploy": "tsx src/deploy.ts",
        "prettier": "prettier assembly//**/*.ts --check && as-prettier --check assembly",
        "prettier:fix": "prettier assembly//**/*.ts --write && as-prettier --write assembly",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "fmt:check": "npm run prettier && npm run lint",
        "fmt": "npm run prettier:fix && npm run lint:fix",
        "doc": "typedoc index.ts --plugin typedoc-plugin-markdown --out docs --skipErrorChecking"
    },
    "keywords": [],
    "license": "MIT",
    "devDependencies": {
        "@as-pect/cli": "^8.1.0",
        "@assemblyscript/loader": "^0.27.22",
        "@massalabs/as-transformer": "^0.3.2",
        "@massalabs/as-types": "^2.0.0",
        "@massalabs/eslint-config": "^0.0.10",
        "@massalabs/massa-as-sdk": "^2.5.5-dev",
        "@massalabs/massa-sc-compiler": "^0.1.1-dev",
        "@massalabs/massa-sc-deployer": "^1.3.0",
        "@massalabs/massa-web3": "^3.0.2",
        "@massalabs/prettier-config-as": "^0.0.2",
        "@massalabs/sc-standards": "0.1.2-dev.20240404142230",
        "@types/node": "^20.10.3",
        "@types/yargs": "^17.0.32",
        "as-bignum": "github:massalabs/as-bignum#0105eb596b2fa707c00712e811a2efdfcb8a9848",
        "assemblyscript": "^0.27.22",
        "dotenv": "^16.3.1",
        "prettier": "^3.1.0",
        "ts-node": "^10.9.1",
        "tslib": "^2.6.2",
        "tsx": "^4.7.0",
        "typedoc": "^0.25.4",
        "typedoc-plugin-markdown": "^3.17.1",
        "typescript": "^5.3.2"
    },
    "type": "module",
    "prettier": "@massalabs/prettier-config-as"
}